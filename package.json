{
	"name": "extract-class",
	"displayName": "extract-class",
	"description": "Extract class refactoring for typescript",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extract-class.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extract-class.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"test": "jest",
		"jest:watch": "jest --watch"
	},
	"dependencies": {
		"lodash": "^4.17.20"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.160",
		"@types/node": "^14.6.0",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^3.10.0",
		"@typescript-eslint/parser": "^3.10.0",
		"eslint": "^7.7.0",
		"jest": "^26.4.2",
		"ts-jest": "^26.2.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	}
}
